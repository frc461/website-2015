<p id="notice"><%= notice %></p>

<h1>Listing Navs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sort index</th>
      <th>Link</th>
      <th>Parent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @navs.each do |nav| %>
      <tr>
        <td><%= nav.name %></td>
        <td><%= nav.sort_index %></td>
        <td><%= nav.link %></td>
        <td><%= nav.parent_id %></td>
        <td><%= link_to 'Show', nav %></td>
        <td><%= link_to 'Edit', edit_nav_path(nav) %></td>
        <td><%= link_to 'Destroy', nav, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nav', new_nav_path %>
